<!DOCTYPE html>
<html>
	<head>
		<title>Sociedad</title>
		<%= stylesheet_link_tag "application", :media => "all" %>
		<%= javascript_include_tag "application" %>
		<%= csrf_meta_tags %>
	</head>
	<body id="sociedad">
		<div id="banner">
			<%= @page_title || t('.titulo') %>
		</div>
		<div id="columns">
			<div id="side">
				<%= yield(:links) %>
			</div>
			<div id="main">
				<p id="notice">
					<%= notice %>
				</p>
				<%= content_for?(:content) ? yield(:content) : yield %>
			</div>
		</div>
	</body>
</html>
